{{#extends 'base'}}
{{#block 'content'}}
<table class="table">
    <thead>
        <tr>
            <th>title</th>
            <th>cost</th>
            <th>description</th>
            <th>date</th>
            <th>stock</th>
            <th>height</th>
            <th>width</th>
        </tr>
    </thead>
    <tbody>
        {{#each posters}}
        <tr>
            <td>{{this.title}}</td>
            <td>{{this.cost}}</td>
            <td>{{this.description}}</td>
            <td>{{this.date}}</td>
            <td>{{this.stock}}</td>
            <td>{{this.height}}</td>
            <td>{{this.width}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>
{{/block}}
{{/extends}}